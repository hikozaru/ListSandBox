<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:ed="http://schemas.microsoft.com/expression/2010/drawing"
	mc:Ignorable="d"
	x:Class="ListBoxSandBox.InsertionCursor"
	x:Name="UserControl"
	d:DesignWidth="7.5" d:DesignHeight="127.25">

	<Grid x:Name="LayoutRoot">
		<Grid Margin="-4,0">
			<ed:RegularPolygon Fill="#FF212121" HorizontalAlignment="Center" Height="10" InnerRadius="1" PointCount="3" Stretch="Fill" Stroke="Black" VerticalAlignment="Top" Width="10" RenderTransformOrigin="0.5,0.5" StrokeThickness="0">
				<ed:RegularPolygon.RenderTransform>
					<TransformGroup>
						<ScaleTransform ScaleY="-1" ScaleX="1"/>
						<SkewTransform AngleY="0" AngleX="0"/>
						<RotateTransform Angle="0"/>
						<TranslateTransform/>
					</TransformGroup>
				</ed:RegularPolygon.RenderTransform>
			</ed:RegularPolygon>
			<Rectangle Fill="#FF212121" HorizontalAlignment="Center" Margin="0,8" Stroke="#FF212121" Width="2" StrokeThickness="0"/>
			<ed:RegularPolygon Fill="#FF212121" HorizontalAlignment="Center" Height="10" InnerRadius="1" PointCount="3" Stretch="Fill" Stroke="#FF212121" VerticalAlignment="Bottom" Width="10"/>
		</Grid>
	</Grid>
</UserControl>